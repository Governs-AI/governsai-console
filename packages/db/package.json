{
  "name": "@governs-ai/db",
  "main": "dist/index.js",
  "types": "index.ts",
  "scripts": {
    "generate": "prisma generate --schema=./schema.prisma",
    "build": "prisma generate --schema=./schema.prisma && sleep 1 && tsc --noEmitOnError false --skipLibCheck --outDir dist && cp -r queries dist/",
    "dev": "tsc --watch",
    "db:generate": "prisma generate --schema=./schema.prisma",
    "db:push": "prisma db push --schema=./schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./schema.prisma",
    "db:studio": "prisma studio --schema=./schema.prisma",
    "db:seed": "tsx seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@simplewebauthn/browser": "^13.2.0",
    "@types/ws": "^8.18.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "prisma": "5.22.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
